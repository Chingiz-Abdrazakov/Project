[32m+[m	[32mAddress a = 4;[m[41m[m
[32m+[m	[32mbyte b1 = 0xcb;[m[41m[m
[32m+[m	[32mb0 = 0x0a;[m[41m[m
[32m+[m	[32mword w = 0xcb0a;[m[41m[m
[32m+[m	[32mb_write(a, b0);[m[41m[m
[32m+[m	[32mb_write(a + 1, b1);[m[41m[m
[32m+[m	[32mword wres = w_read(a);[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mprintf("ww/br \t %04hx = %02hhx%02hhx\n", wres, b1, b0);[m[41m[m
[32m+[m	[32massert(wres == w);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
