[32m+[m	[32mreturn mem[adr];[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m//word read[m[41m[m
[32m+[m[32mword w_read(Address adr){[m[41m[m
[32m+[m	[32mword w = ((word)mem[adr + 1]) << 8;[m[41m[m
[32m+[m	[32m//printf("w = %x\n", w);[m[41m[m
[32m+[m	[32mw = w | mem[adr];[m[41m[m
[32m+[m	[32mreturn w;[m[41m[m
[32m+[m[32m}[m[41m[m
