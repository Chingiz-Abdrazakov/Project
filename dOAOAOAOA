[32m+[m[32mvoid test_mem() {[m[41m[m
[32m+[m	[32mbyte b0 = 0x0a;[m[41m[m
[32m+[m	[32m//write a byte, read a byte[m[41m[m
[32m+[m	[32mb_write(2, b0);[m[41m[m
